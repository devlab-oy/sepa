		<wsdl:operation name="revokeCertificate">
			<soap:operation soapAction=""/>
			<wsdl:input name="revokeCertificateRequest">
				<soap:body use="literal"/>
			</wsdl:input>
			<wsdl:output name="revokeCertificateResponse">
